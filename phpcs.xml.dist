<?xml version="1.0"?>
<ruleset name="PSR12">
  <description>The PSR12 coding standard.</description>
  <autoload>vendor/autoload.php</autoload>
  <rule ref="PSR12"/>
  <file>src/</file>
  <file>tests/</file>
  <arg value="p"/>
  <arg value="s"/>
  <config name="ignore_warnings_on_exit" value="1"/>
</ruleset>
