{
    "name": "emadlebda/coin-market-cap-sdk-php",
    "description": "CoinMarketCap SDK PHP",
    "version": "0.0.0",
    "type": "library",
    "keywords": [
        "coin",
        "market",
        "cap",
        "crypto",
        "stoks",
    ],
    "homepage": "https://github.com/emadlebda/coin-market-cap-sdk-php",
    "authors": [
        {
            "name": "Imad Abulibda",
            "email": "emadexodus@gmail.com"
        },
    ],
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyze --memory-limit 1G",
        "phpunit": "vendor/bin/phpunit",
        "phpcs": "vendor/bin/phpcs",
        "publish": "vendor/bin/publish"
    },
    "autoload": {
        "psr-4": {
            "CoinMarketCap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CoinMarketCap\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*",
        "doctrine/annotations": "^2.0",
        "guzzlehttp/guzzle": "^6.0 || ^7.0",
        "psr/log": "^1.1 || ^2.0 || ^3.0",
        "symfony/cache": "^5.4 || ^6.0",
        "symfony/http-foundation": "^5.0 || ^6.0",
        "symfony/polyfill-php80": "^1.20",
        "symfony/property-access": "^5.4 || ^6.0",
        "symfony/property-info": "^5.4 || ^6.0",
        "symfony/serializer": "^5.4 || ^6.0",
        "symfony/validator": "^5.4 || ^6.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.8",
        "php-coveralls/php-coveralls": "^2.1",
        "phpstan/phpstan": "^0.12 || ^1.0",
        "phpunit/phpunit": "^8.0 || ^9.0 || ^10.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/var-dumper": "^5.4 || ^6.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
